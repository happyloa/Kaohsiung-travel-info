    "@types/aos": "^3.0.7",
    "svelte": "^5.53.1",
    "svelte-check": "^4.4.3",
    "tailwindcss": "^4.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.2.0",
    "aos": "^2.3.4"
  },
  "overrides": {
    "cookie": ">=0.7.0"
  }
}
